<odoo>
    <record model="product.state.info" id="product_state_draft">
        <field name="name">In Development</field>
        <field name="code">draft</field>
    </record>

    <record model="product.state.info" id="product_state_sellable">
        <field name="name">Normal</field>
        <field name="code">sellable</field>
    </record>

    <record model="product.state.info" id="product_state_end">
        <field name="name">End of Lifecycle</field>
        <field name="code">end</field>
    </record>

    <record model="product.state.info" id="product_state_obsolete">
        <field name="name">Obsolete</field>
        <field name="code">obsolete</field>
    </record>
</odoo>