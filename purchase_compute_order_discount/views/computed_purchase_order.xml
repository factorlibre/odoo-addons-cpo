<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_computed_purchase_order_discount_form" model="ir.ui.view">
            <field name="name">computed.purchase.order.form</field>
            <field name="model">computed.purchase.order</field>
            <field name="inherit_id" ref="purchase_compute_order.view_computed_purchase_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='package_quantity_inv']" position="after">
                    <field name="discount"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
